{
  "name": "domotz_chrome_extension",
  "description": "A Vue.js web extension",
  "version": 0.1,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "new-tab-page",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "tab/tab.html"
  },
  "permissions": [
    "storage", "alarms"
  ]
}
